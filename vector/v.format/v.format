#!/bin/sh
#% Module
#%  description: v.format is an interactive GUI (Graphical User Interface) which enables the user to register external SHAPE file into GRASS and allows to define PostGRASS connection.
#% End

if [ $# -eq 0 ]
then
 sh $GISBASE/etc/v.format.tcl
 exit
fi

if [ "$1" != "@ARGS_PARSED@" ] ; then
  exec $GISBASE/etc/bin/cmd/g.parser "$0" "$@"
fi
