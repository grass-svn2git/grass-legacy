#!/bin/sh

export GMPYPATH=$GISBASE/etc/gmwxp
export GRASS_ICONPATH=$GISBASE/etc/gui/icons

if [ "$HOSTTYPE" = "macintosh" -o "$HOSTTYPE" = "powermac" -o "$HOSTTYPE" = "powerpc" -o "$HOSTTYPE" = "intel-pc" ] ; then
	pythonw "$GISBASE/etc/gmwxp/gism.py" -name gmism_py
else
	python "$GISBASE/etc/gmwxp/gism.py" -name gmism_py
fi






