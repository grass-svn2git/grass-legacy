interface_build {
    {s.to.vect} 1
    {Converts a GRASS sites map to a GRASS vector map.}
    {entry input {Name of input site list:} 0 sites}
    {entry output {Name of new vector file:} 0 vector}
    {entry cat {String field in site_list to use in vector file (default=1):} 0 ""}
    {checkbox -p {Do not prompt for metadata entry} "" -p}
}
