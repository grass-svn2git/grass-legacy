all: \
	$(DEPGISLIB) \
	$(ETC)/ellipse.table \
	$(ETC)/datum.table \
	$(ETC)/datumtransform.table \
	$(ETC)/projections \
	$(ETC)/FIPS.code \
	$(ETC)/state27 \
	$(ETC)/state83
#Required for R-statistical language interface:
	cp $(DEPGISLIB) $(GISBASE)/lib
#Required for R-statistical language interface:
	(cd $(SRC)/include ; cp gis.h datetime.h P_datetime.h gisdefs.h site.h P_site.h $(GISBASE)/include )

$(DEPGISLIB): $(OBJ)
	$(SLIBRULE)

$(ETC)/ellipse.table: ellipse.table
	rm -f $@
	cp $? $@
	chmod +r $@

$(ETC)/datum.table: datum.table
	rm -f $@
	cp $? $@
	chmod +r $@

$(ETC)/datumtransform.table: datumtransform.table
	rm -f $@
	cp $? $@
	chmod +r $@

$(ETC)/FIPS.code: FIPS.code
	rm -f $@
	cp $? $@
	chmod +r $@

$(ETC)/state27: state27
	rm -f $@
	cp $? $@
	chmod +r $@

$(ETC)/state83: state83
	rm -f $@
	cp $? $@
	chmod +r $@

$(ETC)/projections: projections
	rm -f $@
	cp $? $@
	chmod +r $@

