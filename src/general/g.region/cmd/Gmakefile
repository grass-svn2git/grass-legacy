PGM    = g.region

LIBES=$(VECTLIB) $(GPROJLIB) $(GISLIB) $(DATETIMELIB)
DEPLIBS=$(DEPVECTLIB) $(DEPGPROJLIB) $(DEPGISLIB) $(DEPDATETIMELIB)

LIST   = printwindow.o\
	adjust.o\
	main.o\
	zoom.o

$(BIN_CMD)/$(PGM): $(LIST) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(LIST) $(LIBES) $(MATHLIB) $(XDRLIB)

$(DEPGISLIB): #
$(DEPVECTLIB): #
$(DEPDATETIMELIB): #
