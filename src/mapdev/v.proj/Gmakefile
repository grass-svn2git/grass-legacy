PGM=v.proj
OFILES = \
	main.o setenv.o 

SRCS = \
	main.c setenv.c
EXTRA_CFLAGS=$(PROJINC)

LIBES=$(GPROJLIB) $(VECTLIB) $(DIG_ATTLIB) $(GISLIB)
DEPLIBS=$(DEPGPROJLIB) $(DEPVECTLIB) $(DEPDIG_ATTLIB) $(DEPGISLIB)
$(BIN_CMD)/$(PGM): $(OFILES) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(OFILES) $(LIBES) $(MATHLIB) $(XDRLIB)

$(DEPGPROJLIB) $(DEPVECTLIB) $(DEPDIG_ATTLIB) $(DEPGISLIB): # 
