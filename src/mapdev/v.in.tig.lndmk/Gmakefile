PGM=v.in.tig.lndmk

LIBES = $(VECTLIB) $(COORCNVLIB) $(GISLIB) $(VASKLIB) $(COORCNVLIB) $(DATETIMELIB) $(DIG_ATTLIB)
DEPLIBS= $(DEPVECTLIB) $(DEPCOORCNVLIB) $(DEPGISLIB) $(DEPVASKLIB) $(DEPDATETIMELIB) $(DEPDIG_ATTLIB)

EXTRA_CFLAGS = $(DIGITFLAGS)

OFILES = main.o parse_input.o cats_stuff.o tig_control.o tig_record.o \
	info_type.o points.o areas.o lines.o coords.o cmd_parse.o \
	match_text.o

$(BIN_CMD)/$(PGM): $(OFILES) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(OFILES) $(LIBES) $(CURSES) $(MATHLIB) $(XDRLIB)

$(OFILES): globals.h
cats_stuff.o: cats.h
