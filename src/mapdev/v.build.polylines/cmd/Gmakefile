LIBTRANS	= $(LIBDIR)/vect32/libes/libtrans.a

PGM = v.build.polylines
LIST =  main.o \
	line_coords.o \
	local_dig.o \
	walk.o

LIBES=$(VECTLIB) $(LIBTRANS) $(DIG_ATTLIB) $(GISLIB)
DEPLIBS=$(DEPVECTLIB) $(LIBTRANS) $(DEPDIG_ATTLIB) $(DEPGISLIB)
$(BIN_CMD)/$(PGM): $(LIST)  $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(LIST) $(LIBES) $(MATHLIB) $(XDRLIB)

$(DEPGISLIB) $(DEPVECTLIB): #

main.o:	global_vars.h
main.o:	global_includes.h
main.o:	line_coords.h
line_coords.o:	line_coords.h
local_dig.o:	local_dig.h
walk.o:	walk.h
walk.o:	line_coords.h
