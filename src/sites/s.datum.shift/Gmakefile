PGM=s.datum.shift
EXTRA_CFLAGS=$(PROJINC)

OFILES = \
	main.o \
	dat.o

LIBES = $(GPROJLIB) $(COORCNVLIB) $(GISLIB) $(DATETIMELIB)
DEPLIBS= $(DEPGPROJLIB) $(DEPCOORCNVLIB) $(DEPGISLIB) $(DEPDATETIMELIB)

$(BIN_CMD)/$(PGM): $(OFILES) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(OFILES) $(LIBES) $(MATHLIB) $(XDRLIB)

$(DEPCOORCNVLIB): #
$(DEPGISLIB): #
$(DEPDATETIMELIB): #
$(DEPGPROJLIB): #
