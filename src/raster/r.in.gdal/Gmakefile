PGM    = r.in.gdal

EXTRA_CFLAGS = $(GDAL_CFLAGS) $(PROJINC)

LIST   = main.o gbgetsymbol.o gdalbridge.o

LIBES= $(GPROJLIB) $(GISLIB) $(IMAGERYLIB) $(GMATHLIB)
DEPLIBS= $(DEPGPROJLIB) $(DEPGISLIB) $(DEPIMAGERYLIB) $(DEPGMATHLIB)
$(BIN_CMD)/$(PGM): $(LIST) $(DEPGISLIB)
	$(CC) $(LDFLAGS) -o $@ $(LIST) \
		$(GDAL_LIBS) $(LIBES) $(MATHLIB) $(XDRLIB)

$(DEPGISLIB): #
