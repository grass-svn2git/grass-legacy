PGM    = r.in.gdal

EXTRA_CFLAGS = $(GDAL_CFLAGS) $(PROJINC)

LIST   = main.o gbgetsymbol.o gdalbridge.o make_loc.o

LIBES= $(GPROJLIB) $(GISLIB) $(IMAGERYLIB)
DEPLIBS= $(DEPGPROJLIB) $(DEPGISLIB) $(DEPIMAGERYLIB)
$(BIN_CMD)/$(PGM): $(LIST) $(DEPGISLIB)
	$(CC) $(LDFLAGS) -o $@ $(LIST) \
		$(GDAL_LIBS) $(LIBES) $(MATHLIB) $(XDRLIB)

$(DEPGISLIB): #
