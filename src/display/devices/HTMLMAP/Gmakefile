EXTRA_CFLAGS = -I../lib

GRAPH = 

OFILES =  \
	Can_do.o\
	Color.o\
	Draw_line.o\
	Get_w_box.o\
	Get_w_line.o\
	Get_w_pnt.o\
	Graph_Clse.o\
	Graph_Set.o\
	Panel.o\
	Polygn_abs.o\
	Reset_clr.o \
	Text.o

all: $(GISBASE)/driver/HTMLMAP
	@if egrep '^HTMLMAP' $(ETC)/monitorcap >/dev/null 2>&1 ; then \
	echo HTMLMAP already defined in $(ETC)/monitorcap ; \
	else \
	echo adding HTMLMAP driver to $(ETC)/monitorcap ; \
	cat moncap.entry >> $(ETC)/monitorcap ; \
	fi

$(GISBASE)/driver/HTMLMAP: $(OFILES) $(DEPDRIVERLIB) $(DEPGISLIB)
	$(CC) $(LDFLAGS) -o $(GISBASE)/driver/HTMLMAP $(OFILES) $(DRIVERLIB) $(GRAPH) $(GISLIB) $(MATHLIB) $(XDRLIB)

$(DEPDRIVERLIB): #
$(DEPGISLIB): #

Graph_Clse.o Graph_Set.o Polygn_abs.o Reset_clr.o Text.o: htmlmap.h
