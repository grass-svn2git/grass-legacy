PGM    = d.what.db

LIBES  = $(DISPLAYLIB) $(RASTERLIB) $(DBMILIB) $(GISLIB) $(DATETIMELIB)
DEPLIBS= $(DEPDISPLAYLIB) $(DEPRASTERLIB) $(DEPGISLIB) $(DEPDATETIMELIB) $(DEPDBMILIB)

LIST   = main.o \
	 show.o \
	 attr.o

$(BIN_CMD)/$(PGM): $(LIST) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(LIST) $(LIBES) $(MATHLIB) $(XDRLIB)

$(DEPLIBS): #
