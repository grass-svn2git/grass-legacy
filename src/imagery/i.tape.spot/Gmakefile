	rec_type.o\
	tape_item.o\
	tape_name.o\
	text_rec.o\
	unmnt_tape.o\
	header.o\
	para_read.o\
	find_row.o\
	mnt_vol.o

$(BIN_INTER)/$(PGM): $(LIST) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(LIST) $(LIBES) $(CURSES) $(MATHLIB) $(XDRLIB)

$(LIST): tape.h

$(DEPLIBS): 	# in case library changes
