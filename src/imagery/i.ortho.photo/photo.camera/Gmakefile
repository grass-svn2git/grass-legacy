PROGRAM	     = photo.camera
HOME        = $(ETC)/imagery

IMAGERYLIB2  = ../libes/libortho.a
EXTRA_CFLAGS = -I../libes

LIST = main.o mod_cam_info.o
LIBES=$(IMAGERYLIB2) $(IMAGERYLIB) $(GISLIB) $(VASKLIB) $(GMATHLIB)
DEPLIBS=$(IMAGERYLIB2) $(DEPIMAGERYLIB) $(DEPGISLIB) $(DEPVASKLIB) $(DEPGMATHLIB)

$(HOME)/$(PROGRAM): $(LIST) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(LIST) $(LIBES) $(CURSES) $(MATHLIB) $(XDRLIB)

$(IMAGERYLIB2) $(DEPLIBS): #

main.o:	camera.h
