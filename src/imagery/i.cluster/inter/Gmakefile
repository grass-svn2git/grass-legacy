PGM=i.cluster

LIST=main.o ask_files.o ask_parms.o run_cluster.o

LIBES=$(IMAGERYLIB) $(GISLIB) $(VASKLIB) $(GMATHLIB)
DEPLIBS=$(DEPIMAGERYLIB) $(DEPGISLIB) $(DEPVASKLIB) $(DEPGMATHLIB)
$(BIN_INTER)/$(PGM): $(LIST) $(DEPLIBS) 
	$(CC) -o $@ $(LDFLAGS) $(LIST) $(LIBES) $(CURSES) $(MATHLIB) $(XDRLIB)


main.o: global.h
ask_files.o: global.h
ask_parms.o: global.h
run_cluster.o: global.h

$(LIST): global.h

$(DEPIMAGERYLIB) $(DEPGISLIB) $(DEPVASKLIB): #
