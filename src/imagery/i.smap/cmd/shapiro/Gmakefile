LIST = main.o parse.o openfiles.o opencell.o labels.o read_sig.o \
	history.o closefiles.o write_img.o

PGM = i.smap

EXTRA_CFLAGS = -I../bouman/

LIBES = ../bouman/lib.a $(GISLIB) $(IMAGERYLIB) $(GMATHLIB)
DEPLIBS= ../bouman/lib.a $(DEPGISLIB) $(DEPIMAGERYLIB) $(DEPGMATHLIB)

$(BIN_CMD)/$(PGM): $(LIST) $(DEPGISLIB) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(LIST) $(LIBES) $(MATHLIB) $(XDRLIB)

$(LIST): ../bouman/bouman.h
read_block.o: ../bouman/region.h

