LIBES = $(IMAGERYLIB) $(GISLIB) $(VASKLIB) $(GMATHLIB)
DEPLIBS= $(DEPIMAGERYLIB) $(DEPGISLIB) $(DEPVASKLIB) $(DEPGMATHLIB)

LIST = main.o\
	ask_options.o\
	ask_title.o\
	ask_window.o\
	tm_extract.o

PGM = i.tape.tm.fast

$(BIN_INTER)/$(PGM): $(LIST) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(LIST) $(LIBES) $(CURSES) $(MATHLIB) $(XDRLIB)

$(LIST): tape.h

