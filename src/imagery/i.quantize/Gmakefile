PGM = i.quantize

LIBES = $(IMAGERYLIB) $(GISLIB) $(VASKLIB) $(GMATHLIB)
DEPLIBS= $(DEPIMAGERYLIB) $(DEPGISLIB) $(DEPVASKLIB) $(DEPGMATHLIB)

OBJ =\
	main.o\
	ask_levels.o\
	compose.o\
	gen_colors.o \
	median.o

$(BIN_INTER)/$(PGM): ${OBJ}
	$(CC) ${LDFLAGS} -o $@ ${OBJ} ${LIBES} $(CURSES) $(MATHLIB) $(XDRLIB)

$(DEPIMAGERYLIB) $(DEPGISLIB) $(VASK): #
